{
    "version": 2,
    "builds": [
      { "src": "Dockerfile", "use": "@vercel/docker" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/var/www/html/public/index.php"
      }
    ],
    "env": {
      "APP_NAME": "Vercel Laravel",
      "APP_ENV": "production",
      "APP_DEBUG": "false",
      "APP_URL": "https://php-laravel.vercel.app",
      "CACHE_DRIVER": "file",
      "SESSION_DRIVER": "file",
      "QUEUE_CONNECTION": "sync"
    }
  }
  